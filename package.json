{
  "name": "epubjs-rn",
  "version": "0.0.114",
  "repository": "https://github.com/futurepress/epubjs-rn",
  "main": "./components/index.js",
  "scripts": {
    "watch": "npm-run-all -p watch:*",
    "watch:bridge": "webpack -w --config bridge/webpack.config.js",
    "watch:reader": "gulp watch-reader",
    "start": "node EpubReader/node_modules/react-native/local-cli/cli.js start",
    "contents": "webpack --config contents/webpack.config.js",
    "bridge": "webpack -p --config bridge/webpack.config.js",
    "build": "npm run bridge && NODE_ENV=production gulp build",
    "prepublish": "npm run build",
    "flow": "flow"
  },
  "license": "BSD-2-Clause",
  "dependencies": {
    "buffer": "^5.0.7",
    "core-js": "~2.5.3",
    "epubjs": "^0.3.57",
    "events": "^1.1.1",
    "i": "^0.3.5",
    "jszip": "^3.1.3",
    "lodash": "^4.17.4",
    "path-webpack": "0.0.3",
    "react-native-fetch-blob": "^0.10.8",
    "react-native-fs": "^2.5.1",
    "react-native-orientation": "3.0.0",
    "react-native-static-server": "^0.2.3",
    "react-native-wkwebview-reborn-fchasen": "1.8.1",
    "react-native-zip-archive": "~2.2.0",
    "stream": "0.0.2",
    "stream-browserify": "^2.0.1",
    "util": "^0.10.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
