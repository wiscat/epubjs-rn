    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-native": "^1.9.2",
    "babili-webpack-plugin": "0.1.2",
    "buffer-shims": "^1.0.0",
    "flow-bin": "~0.61.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-util": "^3.0.8",
    "npm-run-all": "~4.1.2",
    "react": "~16.2.0",
    "react-native": "~0.52.0",
    "react-test-renderer": "~15.6.1",
    "webpack": "~3.10.0"
